   <script type="text/javascript" language="javascript">
   
            $(document).ready(function() {
                HideDate();
                registerRadiobutton();
                NumberValidation();
            });

             function pageLoad(sender, args) {
                 var isAsyncPostback = Sys.WebForms.PageRequestManager.getInstance().get_isInAsyncPostBack();
                 if (isAsyncPostback) {
                     HideDate();
                     registerRadiobutton();
                     NumberValidation();
                 }
             }

             function HideDate() {
                 document.getElementById("ctl00_uoTblDate").style.display = "none";
             }

             function registerRadiobutton() {

                 $("input:radiobutton[type=radio]").click(function() {
                     $('.radio-button > input:radiobutton[type*=radio]').attr('checked', false);
                     if ($("#<%=ReasonOptionValue.ClientID %>").val() != $(this).val()) {
                         $(this).attr('checked', true)
                         $("#<%=ReasonOptionValue.ClientID %>").val($(this).val());
                     }

                     if ($(this).val() == 'rdbOther') {
                         if ($(this).attr("checked") == true) {
                            
                             $("#<% =txtOtherComment.ClientID %>").removeAttr('disabled');
                             $("#<% =txtOtherComment.ClientID %>").val($("#<% =uoHiddenFieldOldOther.ClientID %>").val());

                         }
                         else {
                             $("#<% =txtOtherComment.ClientID %>").attr('disabled', 'disabled');
                             $('#<% =txtOtherComment.ClientID %>').attr('value', '');
                         }
                     }
                     else {
                         $("#<% =txtOtherComment.ClientID %>").attr('disabled', 'disabled');
                         $('#<% =txtOtherComment.ClientID %>').attr('value', '');

                     }
                 });

                 if ($("#<%=rdbOther.ClientID %>").attr("checked") == true) {
                     $("#<% =txtOtherComment.ClientID %>").removeAttr('disabled');
                 }
                 else {
                     $("#<% =txtOtherComment.ClientID %>").attr('disabled', 'disabled');

                 }
                 
             }


             function NumberValidation() {

                 //called when key is pressed in textbox
                 $("#<%=txtUniqeID.ClientID %>").keypress(function(e) {
                     //if the letter is not digit then display error and don't type anything
                     if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                         //display error message
                         //                         $("#errmsg").html("Digits Only").show().fadeOut("slow");
                         
                         alert("Digits Only!");
                         return false;
                     }
                 });
             }

             function Validatereason(val) {
               


                 if ($("#<% =uoHDFCrewVericationID.ClientID %>").val() > 0) { 
                      if  (confirm($("#<% =uoHiddenFieldAppDecMessage.ClientID %>").val()) ){
                     
                         return true;
                     } else {
                         return false;
                     }
                 } 

                 if (val.id == 'ctl00_NaviPlaceHolder_btnApproved') return true;
                
                 var e = $('.radio-button > input:radiobutton[type*=radio]').is(':checked');

                 if (e == true) {
                     return true;
                 }
                 else {
                    alert('Reason required!') 
                     return false;
                 }
             }


             function OpenPDF() {

                 document.getElementById("OpenPDF").href = "../ReportViewer.aspx?report=Immigration&uID=" + $("#<%=txtUniqeID.ClientID %>").val() + "&LOEControlNo=" + $("#<%=txtLOEControlNo.ClientID %>").val(); //document.getElementById("").value;
                 document.getElementById("OpenPDF").click();
 
             
             } 
             
    </script>
   </ContentTemplate>
   
